    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "jest": "^26.4.2",
    "nodemon": "^1.18.3",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.0.1"
